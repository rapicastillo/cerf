/* Superfish Default skin */
#superfish {width: 100%; height: 30px; z-index: 9999;}
#superfish h2 {
  height: 0;
  overflow: hidden;
  position: absolute;
}


#superfish .menu {
  float: left;
  margin: 0;
}
#superfish .menu a {
  padding: 0;
  text-decoration:none;
}
#superfish .menu a, 
#superfish .menu a:visited  { 
  color: #FFF;
}
#superfish .menu li {
  	width: 192px;
	background: #006cb6;
	height: 30px;
	text-align: center;
	margin: 0 5px 0 0;
	color: white;
	font-weight: bold;
	text-decoration: none;
	text-transform: uppercase;
	font-size: 10pt;
	line-height: 25pt;
	cursor: pointer;
	z-index: 999;
}
#superfish .menu li.last {
	margin-right: 0;
}
#superfish .menu ul {
  padding-top: 8px;
  padding-left: 2px;
  width: 192px;
  zoom: 1;
  filter: alpha(opacity=75);
  opacity: 0.75;
  z-index: 999;
}
#superfish .menu li li {
  background: #006CB6;
  text-align: left;
  font-weight: normal;
  padding-left: 4px;
  margin: 0;
  width: 184px;
  line-height: normal;
  height: auto;
  padding-bottom: 4px;
}
#superfish .menu li li li {
  background: #006CB6;
  text-align: left;
  font-weight: normal;
  line-height: normal;
  height: auto;
  padding-bottom: 4px;
}
#superfish .menu li:hover, 
#superfish .menu li.sfHover,
#superfish .menu a:focus, 
#superfish .menu a:hover, 
#superfish .menu a:active,
#superfish .menu a.active {
  background: #006CB6;
  outline: 0;
	z-index: 999;
}

